{
    "manifest_version": 3,
    "name": "Simple Password Manager",
    "version": "1.0",
    "description": "A simple password manager with copy functionality",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["clipboardWrite"],
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }